{
  "name": "Grocy Scanner",
  "version": "1.0.0",
  "slug": "grocy-scanner",
  "description": "A barcode scanner for Grocy, integrated with Home Assistant.",
  "arch": ["armv7", "arm64", "amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "ports": {
    "5000/tcp": 5000
    "80/tcp": 9080,
    "443/tcp": 9443
  },
  "options": {
    "api_key": "YOUR_API_KEY",
    "grocy_url": "http://your-grocy-instance"
  },
  "schema": {
    "api_key": "str",
    "grocy_url": "str"
  }
}
